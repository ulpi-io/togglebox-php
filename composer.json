{
  "name": "togglebox/sdk",
  "description": "PHP SDK for ToggleBox - Remote Config, Feature Flags, and A/B Experiments",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "togglebox",
    "feature-flags",
    "remote-config",
    "ab-testing",
    "experiments"
  ],
  "authors": [
    {
      "name": "ToggleBox Team",
      "email": "support@togglebox.io"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0",
    "psr/simple-cache": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "ToggleBox\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ToggleBox\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse src --level=8"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
